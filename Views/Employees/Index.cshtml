@model List<WebAppCm.Models.Domain.Employee>
@{
}

<h1>Employees</h1>

<table class="table">
    <thead>
    <th>Id</th>
    <th>Name</th>
    <th>Email</th>
    <th>Salary</th>
    <th>Date Of Birth</th>
    <th>Department</th>
    <th> </th>
    </thead>
    <tbody>
        @foreach (var employee in Model)
        {
            <tr>
                <td>@employee.Id</td>
                <td>@employee.Name</td>
                <td>@employee.Email</td>
                <td>@employee.Salary</td>
                <td>@employee.DateOfBirth.ToString("dd-MM-yyyy")</td>
                <td>@employee.Dipartqment</td>
                <td><a href="Employees/View/@employee.Id">View</a></td>
            </tr>
        }
    </tbody>
</table>